<div class="container">
    <p class="mostpopular" id="mostpopular">{{mostPopular.keyword}}</p>

    <div class="row text-center">
        {{#each mostPopular.artikel as |artikel|}}
        <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4">


            <div class="card " style="width:100% ">
                <span class="notify-badge">{{artikel.cena}}&euro;</span>
                <img alt="Card image" class="card-img-top "
                     src="{{artikel.slika}}">
                <div class="collapse" id="{{#replace " " "_"}}{{artikel.naziv}}{{/replace}}">
                    <div class="card card-body">
                        Alergeni: {{alergeni artikel.alergeni}}
                    </div>
                </div>
                <div class="card-body">
                    <h4 class="card-title">{{artikel.naziv}}</h4>
                    <p class="card-text">{{artikel.opis}}</p>
                    <a class="btn btn-primary"  href="#" data-toggle="modal" data-target="#exampleModal">Naroči</a>
                    <a class="btn btn-primary informacije" data-toggle="collapse" href="#{{#replace " " "_"}}{{artikel.naziv}}{{/replace}}" role="button"
                       aria-expanded="false" aria-controls="{{#replace " " "_"}}{{artikel.naziv}}{{/replace}}">
                        <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-info-circle" fill="currentColor"
                             xmlns="http://www.w3.org/2000/svg">
                            <path fill-rule="evenodd"
                                  d="M8 15A7 7 0 1 0 8 1a7 7 0 0 0 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z"/>
                            <path d="M8.93 6.588l-2.29.287-.082.38.45.083c.294.07.352.176.288.469l-.738 3.468c-.194.897.105 1.319.808 1.319.545 0 1.178-.252 1.465-.598l.088-.416c-.2.176-.492.246-.686.246-.275 0-.375-.193-.304-.533L8.93 6.588z"/>
                            <circle cx="8" cy="4.5" r="1"/>
                        </svg>
                    </a>
                </div>
            </div>
        </div>


    {{/each}}

</div>

</div>


